<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0"
	xmlns="http://javafx.com/javafx/11.0.1"
	xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
	<children>
		<Label layoutX="31.0" layoutY="33.0" text="Calculate Sum" />
		<TextField fx:id="txtNumber1" layoutX="32.0" layoutY="60.0"
			promptText="Fist Number" />
		<TextField fx:id="txtNumber2" layoutX="32.0" layoutY="100.0"
			promptText="Second Number" />
		<Button fx:id="btSum" layoutX="32.0" layoutY="138.0"
			mnemonicParsing="false" onAction="#onBtSumAction" text="Soma" />
		<Label layoutX="31.0" layoutY="180.0" text="Result :" />
		<Label fx:id="labelResult" layoutX="78.0" layoutY="180.0" />
	</children>
</AnchorPane>
